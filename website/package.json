        "react": "^18.3.1",
        "react-dom": "^18.3.1",
        "yaml": "^2.5.1"
    },
    "devDependencies": {
        "@astrojs/check": "^0.9.3",
        "@astrojs/react": "^3.6.2",
        "@astrojs/tailwind": "^5.1.1",
        "@genspectrum/dashboard-components": "^0.6.18",
        "@iconify-json/mdi": "^1.2.0",
        "@iconify-json/mdi-light": "^1.2.0",
        "@iconify/tailwind": "^1.1.3",
        "@types/node": "^20.14.11",
        "@types/react": "^18.3.8",
        "@types/react-dom": "^18.3.0",
