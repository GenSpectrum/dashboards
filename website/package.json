{
    "name": "dashboards",
    "type": "module",
    "version": "0.0.1",
    "scripts": {
        "dev": "astro dev",
        "start": "astro dev",
        "build": "astro build",
        "preview": "astro preview",
        "astro": "astro",
        "format": "prettier --ignore-path \"../.gitignore\" --write \"./**/*.{ts,tsx,json,astro,md,mdx,mjs,cjs}\"",
        "check": "npm run check-format && npm run check-types && npm run check-astro",
        "check-format": "prettier --check \"**/*.{ts,tsx,json,astro,md,mdx,mjs,cjs}\"",
        "check-types": "astro sync && tsc --noEmit",
        "check-astro": "astro check",
        "postinstall": "patch-package"
    },
    "dependencies": {
        "@astrojs/node": "^8.3.2",
        "@auth/core": "^0.34.2",
        "@tanstack/react-query": "^5.51.23",
        "astro": "^4.13.3",
        "auth-astro": "^4.1.2",
        "patch-package": "^8.0.0",
        "react": "^18.3.1",
        "react-dom": "^18.3.1"
    },
    "devDependencies": {
        "@astrojs/check": "^0.9.2",
        "@astrojs/react": "^3.6.2",
        "@astrojs/tailwind": "^5.1.0",
        "@genspectrum/dashboard-components": "^0.6.13",
        "@iconify-json/mdi": "^1.1.68",
        "@iconify-json/mdi-light": "^1.1.11",
        "@iconify/tailwind": "^1.1.2",
        "@types/node": "^20.14.11",
        "@types/react": "^18.3.3",
        "@types/react-dom": "^18.3.0",
        "daisyui": "^4.12.10",
        "prettier": "^3.3.3",
        "prettier-plugin-astro": "^0.14.1",
        "prettier-plugin-tailwindcss": "^0.6.6",
        "tailwindcss": "^3.4.9",
        "typescript": "^5.5.4",
        "zod": "^3.23.8"
    }
}
